{
  "name": "rentpal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:prod": "NODE_ENV=production next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:unit": "vitest run src/components src/lib src/hooks",
    "test:integration": "vitest run src/test/integration",
    "test:performance": "vitest run src/test/performance",
    "test:all": "node scripts/test-runner.js all",
    "test:ci": "node scripts/test-runner.js all",
    "deploy": "bash scripts/deploy-production.sh",
    "deploy:staging": "bash scripts/deploy-production.sh --staging",
    "deploy:analyze": "bash scripts/deploy-production.sh --analyze",
    "generate-sitemap": "node scripts/generate-sitemap.js",
    "optimize-images": "node scripts/optimize-images.js",
    "security-audit": "npm audit --audit-level=moderate",
    "bundle-analyze": "npm run build:analyze && npx @next/bundle-analyzer .next",
    "precommit": "npm run lint && npm run type-check && npm run test:run",
    "prepare": "husky install || true"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@sentry/nextjs": "^10.5.0",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-ui-react": "^0.4.7",
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/supabase-js": "^2.56.0",
    "dotenv": "^17.2.1",
    "next": "15.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.48.0",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitest/coverage-v8": "^2.1.8",
    "@vitest/ui": "^2.1.8",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jsdom": "^25.0.1",
    "msw": "^2.6.8",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^2.1.8"
  }
}
